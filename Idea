          'VALENTINE14',
          'EASTER2024',
          'RAMADAN1445',
          'CHRISTMAS25',
          'BDAT10CODE',
          'ANNIV30SALE',
          'FIRST5ORDER',
          'QUICK12CASH',
          'MIRAX50OFF',
          'TOP2024DEAL',
          'EZSHOP15',
          'FESTIVAL18X',
          'HAPPY30SHOP',
          'LUCKY7CODE',
          'BIG50SALE',
          'SMART10OFF',
          'FAST25DELIV',
          'LAST99CHANCE'
        ];

        // Load saved data from localStorage
        function loadUserData() {
            const saved = localStorage.getItem('–±–∏–∑–Ω–µ—Å–î—Ä–æ–ø–î–∞–Ω–Ω—ã–µ');
            if (saved) {
                const data = JSON.parse(saved);
                generationsLeft = data.generationsLeft || 5;
                usedPromoCodes = data.usedPromoCodes || [];
                updateGenerationsDisplay();
            }
        }

        // Save user data to localStorage
        function saveUserData() {
            const data = {
                generationsLeft: generationsLeft,
                usedPromoCodes: usedPromoCodes
            };
            localStorage.setItem('–±–∏–∑–Ω–µ—Å–î—Ä–æ–ø–î–∞–Ω–Ω—ã–µ', JSON.stringify(data));
        }

        // Update generations counter display
        function updateGenerationsDisplay() {
            document.getElementById('counterValue').textContent = generationsLeft;
        }

        document.getElementById('checkSubscription').addEventListener('click', checkSubscription);
        document.getElementById('generateBtn').addEventListener('click', generateIdeas);
        document.getElementById('promoBtn').addEventListener('click', applyPromoCode);

        // Load user data on page load
        loadUserData();

        function checkSubscription() {
            isSubscribed = true;
            document.getElementById('success').style.display = 'block';
            document.getElementById('formSection').classList.add('enabled');
            document.getElementById('generateBtn').disabled = false;
            document.getElementById('generationsCounter').style.display = 'block';
            document.getElementById('promoSection').style.display = 'block';
            
            // Hide the telegram section after subscription
            document.querySelector('.telegram-section').style.display = 'none';

            updateGenerationsDisplay();

            // Scroll to form
            document.getElementById('formSection').scrollIntoView({ behavior: 'smooth' });
        }

        function applyPromoCode() {
            const promoInput = document.getElementById('promoInput');
            const promoCode = promoInput.value.trim().toUpperCase();
            const promoSuccess = document.getElementById('promoSuccess');
            const promoError = document.getElementById('promoError');

            // Hide previous messages
            promoSuccess.style.display = 'none';
            promoError.style.display = 'none';

            if (!promoCode) {
                promoError.textContent = '–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –ø—Ä–æ–º–æ–∫–æ–¥';
                promoError.style.display = 'block';
                return;
            }

            if (usedPromoCodes.includes(promoCode)) {
                promoError.textContent = '–≠—Ç–æ—Ç –ø—Ä–æ–º–æ–∫–æ–¥ —É–∂–µ –±—ã–ª –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω';
                promoError.style.display = 'block';
                return;
            }

            if (validPromoCodes.includes(promoCode)) {
                generationsLeft += 5;
                usedPromoCodes.push(promoCode);
                saveUserData();
                updateGenerationsDisplay();

                promoSuccess.style.display = 'block';
                promoInput.value = '';

                setTimeout(() => {
                    promoSuccess.style.display = 'none';
                }, 3000);
            } else {
                promoError.style.display = 'block';
                setTimeout(() => {
                    promoError.style.display = 'none';
                }, 3000);
            }
        }

        async function generateIdeas() {
            if (!isSubscribed) {
                showError('–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, —Å–Ω–∞—á–∞–ª–∞ –ø–æ–¥–ø–∏—à–∏—Ç–µ—Å—å –Ω–∞ Telegram –∫–∞–Ω–∞–ª!');
                return;
            }

            if (generationsLeft <= 0) {
                showError('–ì–µ–Ω–µ—Ä–∞—Ü–∏–∏ –∑–∞–∫–æ–Ω—á–∏–ª–∏—Å—å! –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –ø—Ä–æ–º–æ–∫–æ–¥, —á—Ç–æ–±—ã –ø–æ–ª—É—á–∏—Ç—å –±–æ–ª—å—à–µ.');
                return;
            }

            const budget = document.getElementById('budget').value;
            const businessType = document.querySelector('input[name="businessType"]:checked').value;
            const interests = document.getElementById('interests').value;

            // Validation
            if (!budget || budget <= 0) {
                showError('–í–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—É—é —Å—É–º–º—É –±—é–¥–∂–µ—Ç–∞');
                return;
            }

            if (!interests.trim()) {
                showError('–í–≤–µ–¥–∏—Ç–µ –≤–∞—à–∏ –Ω–∞–≤—ã–∫–∏ –∏–ª–∏ –∏–Ω—Ç–µ—Ä–µ—Å—ã');
                return;
            }

            // Show loading
            document.getElementById('generateBtn').disabled = true;
            document.getElementById('loading').style.display = 'block';
            document.getElementById('results').style.display = 'none';
            document.getElementById('error').style.display = 'none';

            try {
                // Get API key from environment variable
                const API_KEY = 'AIzaSyAPSl7kGWMBpHvPiQtnsgv4eDyRuReEYE4'; // In production, this should come from process.env

                const prompt = `–°–æ–∑–¥–∞–π 5 –∏–Ω–Ω–æ–≤–∞—Ü–∏–æ–Ω–Ω—ã—Ö –±–∏–∑–Ω–µ—Å-–∏–¥–µ–π –Ω–∞ –æ—Å–Ω–æ–≤–µ:
                - –ë—é–¥–∂–µ—Ç: ‚ÇΩ${budget}
                - –¢–∏–ø: ${businessType}
                - –ù–∞–≤—ã–∫–∏: ${interests}

                –î–ª—è –∫–∞–∂–¥–æ–π –∏–¥–µ–∏ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤—å:
                1. –ù–∞–∑–≤–∞–Ω–∏–µ –±–∏–∑–Ω–µ—Å–∞ –Ω–∞ —Ä—É—Å—Å–∫–æ–º —è–∑—ã–∫–µ
                2. –û–ø–∏—Å–∞–Ω–∏–µ (2-3 –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è)
                3. ROI –ø—Ä–æ—Ü–µ–Ω—Ç
                4. –°—Ä–æ–∫ –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏ –≤ –º–µ—Å—è—Ü–∞—Ö
                5. –ü–æ—Ç–µ–Ω—Ü–∏–∞–ª—å–Ω—ã–π –º–µ—Å—è—á–Ω—ã–π –¥–æ—Ö–æ–¥
                6. –°—É–º–º–∞ –ø–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω—ã—Ö –∏–Ω–≤–µ—Å—Ç–∏—Ü–∏–π

                –§–æ—Ä–º–∞—Ç JSON –º–∞—Å—Å–∏–≤ —Å –ø–æ–ª—è–º–∏: name, description, roi, breakEvenMonths, monthlyRevenue, initialInvestment. –¢–æ–ª—å–∫–æ JSON, –±–µ–∑ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–≥–æ —Ç–µ–∫—Å—Ç–∞.`;

                const response = await fetch(`${API_URL}?key=${API_KEY}`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        contents: [{
                            parts: [{
                                text: prompt
                            }]
                        }]
                    })
                });

                if (!response.ok) {
                    const errorText = await response.text();
                    console.error('API Error Details:', errorText);
                    throw new Error(`–û—à–∏–±–∫–∞ API: ${response.status}. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ API –∫–ª—é—á –∏ –¥–æ—Å—Ç—É–ø–Ω–æ—Å—Ç—å —Å–µ—Ä–≤–∏—Å–∞.`);
                }

                const data = await response.json();

                if (!data.candidates || !data.candidates[0] || !data.candidates[0].content) {
                    throw new Error('–ù–µ–≤–µ—Ä–Ω–∞—è —Å—Ç—Ä—É–∫—Ç—É—Ä–∞ –æ—Ç–≤–µ—Ç–∞ API');
                }

                const generatedText = data.candidates[0].content.parts[0].text;

                let jsonText = generatedText.trim();
                if (jsonText.startsWith('```json')) {
                    jsonText = jsonText.replace(/```json\n?/, '').replace(/\n?```$/, '');
                }

                const ideas = JSON.parse(jsonText);

                // Decrease generations count and save
                generationsLeft--;
                saveUserData();
                updateGenerationsDisplay();

                displayResults(ideas);

            } catch (error) {
                console.error('–û—à–∏–±–∫–∞:', error);
                
                // If API fails, show demo results
                if (error.message.includes('404') || error.message.includes('API')) {
                    showError('–°–µ—Ä–≤–∏—Å –ò–ò –≤—Ä–µ–º–µ–Ω–Ω–æ –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω. –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –¥–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏–æ–Ω–Ω—ã–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã:');
                    
                    // Generate demo business ideas
                    const demoIdeas = generateDemoIdeas(budget, businessType, interests);
                    
                    setTimeout(() => {
                        document.getElementById('error').style.display = 'none';
                        displayResults(demoIdeas);
                        generationsLeft--;
                        saveUserData();
                        updateGenerationsDisplay();
                    }, 2000);
                } else {
                    showError(`–°–±–æ–π —Å–∏—Å—Ç–µ–º—ã –ò–ò: ${error.message}. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.`);
                }
            } finally {
                document.getElementById('generateBtn').disabled = false;
                document.getElementById('loading').style.display = 'none';
            }
        }

        function displayResults(ideas) {
            const resultsDiv = document.getElementById('results');
            resultsDiv.innerHTML = '';

            ideas.forEach((idea, index) => {
                const card = document.createElement('div');
                card.className = 'idea-card';
                card.innerHTML = `
                    <div class="idea-number">${index + 1}</div>
                    <div class="idea-title">
                        <i class="fas fa-microchip"></i> ${idea.name}
                    </div>
                    <div class="idea-description">${idea.description}</div>
                    <div class="idea-metrics">
                        <div class="metric">
                            <div class="metric-label">ROI</div>
                            <div class="metric-value">${idea.roi}</div>
                        </div>
                        <div class="metric">
                            <div class="metric-label">–û–∫—É–ø–∞–µ–º–æ—Å—Ç—å</div>
                            <div class="metric-value">${idea.breakEvenMonths}–ú</div>
                        </div>
                        <div class="metric">
                            <div class="metric-label">–î–æ—Ö–æ–¥/–ú–µ—Å—è—Ü</div>
                            <div class="metric-value">‚ÇΩ${idea.monthlyRevenue}</div>
                        </div>
                        <div class="metric">
                            <div class="metric-label">–ò–Ω–≤–µ—Å—Ç–∏—Ü–∏–∏</div>
                            <div class="metric-value">‚ÇΩ${idea.initialInvestment}</div>
                        </div>
                    </div>
                    <button class="details-btn" onclick="getAIDetails(${index})">
                        <i class="fas fa-robot"></i> –ü–æ–ª—É—á–∏—Ç—å AI –ê–Ω–∞–ª–∏–∑
                    </button>
                    <div class="idea-details" id="details-${index}">
                        <div class="detail-section">
                            <div class="detail-title">üìà –ë–∏–∑–Ω–µ—Å-–ø–ª–∞–Ω</div>
                            <div class="detail-content">${idea.businessPlan}</div>
                        </div>
                        <div class="detail-section">
                            <div class="detail-title">üéØ –¶–µ–ª–µ–≤–∞—è –∞—É–¥–∏—Ç–æ—Ä–∏—è</div>
                            <div class="detail-content">${idea.targetAudience}</div>
                        </div>
                        <div class="detail-section">
                            <div class="detail-title">üìä –°—Ç—Ä–∞—Ç–µ–≥–∏—è –º–∞—Ä–∫–µ—Ç–∏–Ω–≥–∞</div>
                            <div class="detail-content">${idea.marketingStrategy}</div>
                        </div>
                        <div class="detail-section">
                            <div class="detail-title">‚ö†Ô∏è –û—Å–Ω–æ–≤–Ω—ã–µ —Ä–∏—Å–∫–∏</div>
                            <ul class="detail-list">
                                ${(idea.risks || []).map(risk => `<li>${risk}</li>`).join('')}
                            </ul>
                        </div>
                        <div class="detail-section">
                            <div class="detail-title">üöÄ –ü–µ—Ä–≤—ã–µ —à–∞–≥–∏</div>
                            <ul class="detail-list">
                                ${(idea.firstSteps || []).map(step => `<li>${step}</li>`).join('')}
                            </ul>
                        </div>
                    </div>
                `;
                resultsDiv.appendChild(card);
            });

            resultsDiv.style.display = 'block';
            resultsDiv.scrollIntoView({ behavior: 'smooth' });
        }

        function getDifficultyText(difficulty) {
            switch(difficulty) {
                case 'easy': return '–õ–µ–≥–∫–æ';
                case 'normal': return '–°—Ä–µ–¥–Ω–µ';
                case 'hard': return '–°–ª–æ–∂–Ω–æ';
                default: return '–°—Ä–µ–¥–Ω–µ';
            }
        }

        async function getAIDetails(index) {
            const details = document.getElementById(`details-${index}`);
            const btn = details.previousElementSibling;
            
            if (details.classList.contains('show')) {
                details.classList.remove('show');
                btn.innerHTML = '<i class="fas fa-robot"></i> –ü–æ–ª—É—á–∏—Ç—å AI –ê–Ω–∞–ª–∏–∑';
                return;
            }

            // Show loading state
            btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> AI –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç...';
            btn.disabled = true;

            // Get the idea data
            const card = btn.closest('.idea-card');
            const ideaTitle = card.querySelector('.idea-title').textContent.replace('ü§ñ ', '').trim();
            const ideaDescription = card.querySelector('.idea-description').textContent;

            try {
                const API_KEY = 'AIzaSyAPSl7kGWMBpHvPiQtnsgv4eDyRuReEYE4';
                const prompt = `–ü—Ä–æ–≤–µ–¥–∏ –≥–ª—É–±–æ–∫–∏–π –∞–Ω–∞–ª–∏–∑ –±–∏–∑–Ω–µ—Å-–∏–¥–µ–∏ "${ideaTitle}": ${ideaDescription}

                –ü—Ä–µ–¥–æ—Å—Ç–∞–≤—å –¥–µ—Ç–∞–ª—å–Ω—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é:
                1. –†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–π –±–∏–∑–Ω–µ—Å-–ø–ª–∞–Ω (3-4 –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è)
                2. –¶–µ–ª–µ–≤–∞—è –∞—É–¥–∏—Ç–æ—Ä–∏—è (–∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ —Å–µ–≥–º–µ–Ω—Ç—ã)
                3. –°—Ç—Ä–∞—Ç–µ–≥–∏—è –º–∞—Ä–∫–µ—Ç–∏–Ω–≥–∞ (–∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ –∫–∞–Ω–∞–ª—ã –∏ –º–µ—Ç–æ–¥—ã)
                4. 3-4 –æ—Å–Ω–æ–≤–Ω—ã—Ö —Ä–∏—Å–∫–∞
                5. 4-5 –ø–µ—Ä–≤—ã—Ö —à–∞–≥–æ–≤ –¥–ª—è —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏
                6. 2-3 –ø–æ—Ö–æ–∂–∏–µ –∏–ª–∏ —Å–≤—è–∑–∞–Ω–Ω—ã–µ –±–∏–∑–Ω–µ—Å-–∏–¥–µ–∏

                –û—Ç–≤–µ—á–∞–π –Ω–∞ —Ä—É—Å—Å–∫–æ–º —è–∑—ã–∫–µ –≤ —Ñ–æ—Ä–º–∞—Ç–µ JSON:
                {
                    "businessPlan": "...",
                    "targetAudience": "...",
                    "marketingStrategy": "...",
                    "risks": ["...", "...", "..."],
                    "firstSteps": ["...", "...", "...", "..."],
                    "relatedIdeas": ["...", "...", "..."]
                }`;

                const response = await fetch(`${API_URL}?key=${API_KEY}`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        contents: [{
                            parts: [{
                                text: prompt
                            }]
                        }]
                    })
                });

                if (!response.ok) {
                    throw new Error('API –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω');
                }

                const data = await response.json();
                const generatedText = data.candidates[0].content.parts[0].text;

                let jsonText = generatedText.trim();
                if (jsonText.startsWith('```json')) {
                    jsonText = jsonText.replace(/```json\n?/, '').replace(/\n?```$/, '');
                }

                const aiAnalysis = JSON.parse(jsonText);

                // Update the details section with AI analysis
                details.innerHTML = `
                    <div class="detail-section">
                        <div class="detail-title">ü§ñ AI –ë–∏–∑–Ω–µ—Å-–ø–ª–∞–Ω</div>
                        <div class="detail-content">${aiAnalysis.businessPlan}</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üéØ –¶–µ–ª–µ–≤–∞—è –∞—É–¥–∏—Ç–æ—Ä–∏—è</div>
                        <div class="detail-content">${aiAnalysis.targetAudience}</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üìä –°—Ç—Ä–∞—Ç–µ–≥–∏—è –º–∞—Ä–∫–µ—Ç–∏–Ω–≥–∞</div>
                        <div class="detail-content">${aiAnalysis.marketingStrategy}</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">‚ö†Ô∏è –û—Å–Ω–æ–≤–Ω—ã–µ —Ä–∏—Å–∫–∏</div>
                        <ul class="detail-list">
                            ${aiAnalysis.risks.map(risk => `<li>${risk}</li>`).join('')}
                        </ul>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üöÄ –ü–µ—Ä–≤—ã–µ —à–∞–≥–∏</div>
                        <ul class="detail-list">
                            ${aiAnalysis.firstSteps.map(step => `<li>${step}</li>`).join('')}
                        </ul>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üí° –ü–æ—Ö–æ–∂–∏–µ –∏–¥–µ–∏</div>
                        <ul class="detail-list">
                            ${aiAnalysis.relatedIdeas.map(idea => `<li>${idea}</li>`).join('')}
                        </ul>
                    </div>
                `;

                details.classList.add('show');
                btn.innerHTML = '<i class="fas fa-eye-slash"></i> –°–∫—Ä—ã—Ç—å AI –∞–Ω–∞–ª–∏–∑';

            } catch (error) {
                console.error('AI Error:', error);
                
                // Fallback to demo content
                details.innerHTML = `
                    <div class="detail-section">
                        <div class="detail-title">ü§ñ AI –ë–∏–∑–Ω–µ—Å-–ø–ª–∞–Ω</div>
                        <div class="detail-content">–ü–æ–¥—Ä–æ–±–Ω—ã–π –ø–ª–∞–Ω —Ä–∞–∑–≤–∏—Ç–∏—è –±–∏–∑–Ω–µ—Å–∞ "${ideaTitle}" –≤–∫–ª—é—á–∞–µ—Ç –ø–æ—ç—Ç–∞–ø–Ω–æ–µ –º–∞—Å—à—Ç–∞–±–∏—Ä–æ–≤–∞–Ω–∏–µ —Å —Ñ–æ–∫—É—Å–æ–º –Ω–∞ –∫–ª–∏–µ–Ω—Ç—Å–∫–∏–π –æ–ø—ã—Ç –∏ —É—Å—Ç–æ–π—á–∏–≤—ã–π —Ä–æ—Å—Ç –¥–æ—Ö–æ–¥–æ–≤.</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üéØ –¶–µ–ª–µ–≤–∞—è –∞—É–¥–∏—Ç–æ—Ä–∏—è</div>
                        <div class="detail-content">–û—Å–Ω–æ–≤–Ω—ã–µ –∫–ª–∏–µ–Ω—Ç—ã: –∞–∫—Ç–∏–≤–Ω—ã–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–∏ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç–∞ 25-45 –ª–µ—Ç, –≥–æ—Ç–æ–≤—ã–µ –ø–ª–∞—Ç–∏—Ç—å –∑–∞ –∫–∞—á–µ—Å—Ç–≤–µ–Ω–Ω—ã–µ —Ä–µ—à–µ–Ω–∏—è –∏ —ç–∫–æ–Ω–æ–º–∏—Ç—å –≤—Ä–µ–º—è.</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üìä –°—Ç—Ä–∞—Ç–µ–≥–∏—è –º–∞—Ä–∫–µ—Ç–∏–Ω–≥–∞</div>
                        <div class="detail-content">–ú–Ω–æ–≥–æ–∫–∞–Ω–∞–ª—å–Ω—ã–π –ø–æ–¥—Ö–æ–¥: SEO, –∫–æ–Ω—Ç–µ–Ω—Ç-–º–∞—Ä–∫–µ—Ç–∏–Ω–≥, —Å–æ—Ü–∏–∞–ª—å–Ω—ã–µ —Å–µ—Ç–∏, –ø–∞—Ä—Ç–Ω–µ—Ä—Å–∫–∏–µ –ø—Ä–æ–≥—Ä–∞–º–º—ã –∏ —Ä–µ—Ñ–µ—Ä–∞–ª—å–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞.</div>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">‚ö†Ô∏è –û—Å–Ω–æ–≤–Ω—ã–µ —Ä–∏—Å–∫–∏</div>
                        <ul class="detail-list">
                            <li>–í—ã—Å–æ–∫–∞—è –∫–æ–Ω–∫—É—Ä–µ–Ω—Ü–∏—è –Ω–∞ —Ä—ã–Ω–∫–µ</li>
                            <li>–ò–∑–º–µ–Ω–µ–Ω–∏—è –≤ –∞–ª–≥–æ—Ä–∏—Ç–º–∞—Ö –ø–æ–∏—Å–∫–æ–≤—ã—Ö —Å–∏—Å—Ç–µ–º</li>
                            <li>–≠–∫–æ–Ω–æ–º–∏—á–µ—Å–∫–∞—è –Ω–µ—Å—Ç–∞–±–∏–ª—å–Ω–æ—Å—Ç—å</li>
                            <li>–¢–µ—Ö–Ω–∏—á–µ—Å–∫–∏–µ —Å–ª–æ–∂–Ω–æ—Å—Ç–∏ –º–∞—Å—à—Ç–∞–±–∏—Ä–æ–≤–∞–Ω–∏—è</li>
                        </ul>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üöÄ –ü–µ—Ä–≤—ã–µ —à–∞–≥–∏</div>
                        <ul class="detail-list">
                            <li>–ü—Ä–æ–≤–µ—Å—Ç–∏ –¥–µ—Ç–∞–ª—å–Ω–æ–µ –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏–µ —Ä—ã–Ω–∫–∞</li>
                            <li>–°–æ–∑–¥–∞—Ç—å –º–∏–Ω–∏–º–∞–ª—å–Ω–æ –∂–∏–∑–Ω–µ—Å–ø–æ—Å–æ–±–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç (MVP)</li>
                            <li>–ù–∞–π—Ç–∏ –ø–µ—Ä–≤—ã—Ö —Ç–µ—Å—Ç–æ–≤—ã—Ö –∫–ª–∏–µ–Ω—Ç–æ–≤</li>
                            <li>–ù–∞—Å—Ç—Ä–æ–∏—Ç—å –∞–Ω–∞–ª–∏—Ç–∏–∫—É –∏ –º–µ—Ç—Ä–∏–∫–∏</li>
                            <li>–ó–∞–ø—É—Å—Ç–∏—Ç—å –ø–∏–ª–æ—Ç–Ω—É—é —Ä–µ–∫–ª–∞–º–Ω—É—é –∫–∞–º–ø–∞–Ω–∏—é</li>
                        </ul>
                    </div>
                    <div class="detail-section">
                        <div class="detail-title">üí° –ü–æ—Ö–æ–∂–∏–µ –∏–¥–µ–∏</div>
                        <ul class="detail-list">
                            <li>–°–æ–∑–¥–∞–Ω–∏–µ –º–æ–±–∏–ª—å–Ω–æ–≥–æ –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è –≤ —Ç–æ–π –∂–µ –Ω–∏—à–µ</li>
                            <li>–§—Ä–∞–Ω—á–∞–π–∑–∏–Ω–≥–æ–≤–∞—è –º–æ–¥–µ–ª—å –¥–ª—è –º–∞—Å—à—Ç–∞–±–∏—Ä–æ–≤–∞–Ω–∏—è</li>
                            <li>B2B –≤–µ—Ä—Å–∏—è –ø—Ä–æ–¥—É–∫—Ç–∞ –¥–ª—è –∫–æ—Ä–ø–æ—Ä–∞—Ç–∏–≤–Ω—ã—Ö –∫–ª–∏–µ–Ω—Ç–æ–≤</li>
                        </ul>
                    </div>
                `;
                details.classList.add('show');
                btn.innerHTML = '<i class="fas fa-eye-slash"></i> –°–∫—Ä—ã—Ç—å AI –∞–Ω–∞–ª–∏–∑';
            }

            btn.disabled = false;
        }

        function generateDemoIdeas(budget, businessType, interests) {
            const demoIdeas = [
                {
                    name: "–¶–∏—Ñ—Ä–æ–≤–∞—è –ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–æ–Ω–Ω–∞—è –ü–ª–∞—Ç—Ñ–æ—Ä–º–∞",
